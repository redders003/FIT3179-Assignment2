{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 300, "height": 200,
    "data": {"url": "https://raw.githubusercontent.com/redders003/FIT3179-Assignment2/refs/heads/main/Data/Updated_AusBudgetSpending.csv"},
    "mark": "area",
    "encoding": {
      "x": {
        "timeUnit": "year", "field": "year",
        "axis": {"format": "%Y"}
      },
      "y": {
        "aggregate": "sum", "field": "count"
      },
      "color": {
        "field": "series",
        "scale": {"scheme": "category20b"}
      }
    }
  }
  
  