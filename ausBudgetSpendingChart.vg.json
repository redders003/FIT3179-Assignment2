{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 300,
    "height": 200,
    "data": {
      "url": "https://raw.githubusercontent.com/redders003/FIT3179-Assignment2/refs/heads/main/Data/Updated_AusBudgetSpending.csv"
    },
    "transform": [
      {
        "fold": ["spendInAus", "spendOutAus"],
        "as": ["series", "count"]
      }
    ],
    "mark": "area",
    "encoding": {
      "x": {
        "field": "year",
        "type": "temporal",
        "timeUnit": "year",
        "axis": {
            "title": "",
            "tickCount": "year"
        }
      },
      "y": {
        "aggregate": "sum",
        "field": "count",
        "type": "quantitative",
        "axis": {
          "title": "Spending"
        }
      },
      "color": {
        "field": "series",
        "type": "nominal",
        "legend": {
          "title": "Spending Type"
        }
      }
    }
  }
  